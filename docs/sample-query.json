{
  "keys": ["tags.Name", "instance_type", "state", "region"],
  "filters": [
    {"id": "is_stopped", "key": "state",  "not_matches": ["running"], "regex": false},
    {"id": "is_east_2", "key": "region", "matches": ["east"], "not_matches": ["east-1"], "regex": true}
  ],
  "filter_type": "AND",
  "maxrecords": -1,
  "reverse_match": false,
  "sort_on": "tags.Name",
  "reverse": false,
  "summary": false
}
